  "scripts": {
    "postinstall": "if test \"$NODE_ENV\" = \"production\"; then npm run build; fi",
    "build": "cross-env NODE_ENV=production webpack --config internals/webpack/webpack.prod.js --color -p",
    "prebuild": "npm run clean:build",
    "dev": "cross-env NODE_ENV=development node -r dotenv-safe/config server",
    "start": "cross-env NODE_ENV=production node -r dotenv-safe/config server",
    "lint": "npm run lint:js --silent && npm run lint:css --silent",
    "lint:js": "npm run lint:eslint --silent && npm run lint:jscs --silent && npm run lint:flow --silent",
    "lint:js:staged": "npm run lint:eslint:staged --silent",
    "lint:eslint": "eslint . --ignore-path .gitignore",
    "lint:eslint:staged": "eslint-staged",
    "lint:jscs": "jscs .",
    "lint:jscs:staged": "jscs-staged",
    "lint:flow": "flow check",
    "lint:flow:staged": "flow-staged",
    "lint:css": "stylelint ./app/**/*.css",
    "lint:css:staged": "stylelint-staged",
    "lint:staged": "npm run lint:js:staged --silent && npm run lint:jscs:staged --silent && npm run lint:flow:staged --silent && npm run lint:css:staged --silent",
    "test": "cross-env NODE_ENV=test karma start internals/testing/karma.conf.js --single-run",
    "test:watch": "npm run test -- --auto-watch --no-single-run",
    "test:firefox": "npm run test -- --browsers Firefox",
    "test:safari": "npm run test -- --browsers Safari",
    "test:ie": "npm run test -- --browsers IE",
    "pretest": "npm run clean:coverage",
    "coveralls": "cat ./coverage/lcov/lcov.info | coveralls",
    "clean:build": "rimraf ./build/*",
    "clean:coverage": "rimraf ./coverage/*",
    "clean": "npm run clean:build && npm run clean:coverage"
  },
  "engines": {
    "node": "6.0.0",
    "npm": "3.8.9"
  },
  "pre-commit": "lint:staged",
