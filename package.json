  "scripts": {
    "postinstall": "yarn run dll",
    "build": "yarn run build:server && yarn run build:renderer",
    "build:server": "cross-env NODE_ENV=production babel server --presets es2017,stage-0 --out-dir build/server",
    "build:renderer": "cross-env NODE_ENV=production webpack --config internals/webpack/webpack.prod.js --color -p",
    "prebuild": "yarn run clean:build",
    "dll": "cross-env NODE_ENV=development webpack --config internals/webpack/webpack.dll.js --color -p",
    "predll": "yarn run clean:dll",
    "dev": "cross-env NODE_ENV=development nodemon server --watch server --exec babel-node --presets es2017,stage-0",
    "dev:cockpit": "cross-env NODE_ENV=development webpack-dashboard -p 9838 -- nodemon server --watch server --exec babel-node --presets es2017,stage-0",
    "predev": "docker-compose up -d",
    "start:dev": "yarn run db:setup && yarn run start",
    "start": "cross-env NODE_ENV=production node build/server",
    "cards": "reactcards -p 3001 -e app/entry/cards -c internals/webpack/webpack.cards.js",
    "cards:cockpit": "webpack-dashboard -p 9839 -- reactcards -p 3001 -e app/entry/cards -c internals/webpack/webpack.cards.js",
    "lint": "yarn run lint:js && yarn run lint:css ",
    "lint:js": "yarn run lint:eslint && yarn run lint:flow",
    "lint:eslint": "eslint ./app ./server",
    "lint:flow": "flow check",
    "lint:css": "stylelint ./app/**/*.css",
    "lint:staged": "lint-staged",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "test:watch": "yarn run test -- --watch",
    "clean:build": "rimraf ./build/*",
    "clean:coverage": "rimraf ./coverage/*",
    "clean:dll": "rimraf ./dll/*",
    "clean": "yarn run clean:build && yarn run clean:coverage && yarn run clean:dll",
    "db:drop": "babel-node internals/scripts/dropDatabase --presets es2017,stage-0",
    "db:setup": "babel-node internals/scripts/setupDatabase --presets es2017,stage-0",
    "db:reset": "yarn run db:drop && yarn run db:setup"
  },
  "engines": {
    "node": "6.7.0"
  },
  "lint-staged": {
    "lint:eslint": "*.js",
    "lint:flow": "*.js",
    "lint:css": "*.css"
  },
  "config": {
    "ghooks": {
      "pre-commit": "yarn run lint:staged",
      "post-checkout": "./internals/git/post-checkout",
      "post-merge": "./internals/git/post-merge",
      "post-rewrite": "./internals/git/post-rewrite"
    }
  },
  "dll": {
    "exclude": [
      "babel-cli",
      "babel-core",
      "babel-loader",
      "babel-plugin-react-transform",
      "babel-polyfill",
      "babel-preset-es2015",
      "babel-preset-es2017",
      "babel-preset-react",
      "babel-preset-react-optimize",
      "babel-preset-stage-0",
      "babel-relay-plugin",
      "compression",
      "cross-env",
      "css-loader",
      "deasync",
      "dotenv",
      "dotenv-extended",
      "exports-loader",
      "express",
      "express-graphql",
      "extract-text-webpack-plugin",
      "faker",
      "file-loader",
      "git-rev-sync",
      "graphiql",
      "graphql",
      "graphql-relay",
      "graphql-union-input-type",
      "html-webpack-plugin",
      "http-proxy-middleware",
      "imports-loader",
      "json-loader",
      "mongoose",
      "offline-plugin",
      "postcss-color-function",
      "postcss-cssnext",
      "postcss-focus",
      "postcss-for",
      "postcss-import",
      "postcss-loader",
      "postcss-nested",
      "postcss-reporter",
      "raw-loader",
      "rimraf",
      "style-loader",
      "transform-loader",
      "url-loader",
      "webpack",
      "webpack-md5-hash"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "^.+\\.css$": "<rootDir>/internals/testing/environment/mock.style.js",
      "^.+\\.(jpe?g|png|gif|svg|woff2?|ttf|eot)$": "<rootDir>/internals/testing/environment/mock.file.js"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/internals/testing/setup.js",
    "testPathIgnorePatterns": [
      "node_modules"
    ],
    "testRegex": "/__tests__/.*\\.test.js$",
    "moduleDirectories": [
      "screens",
      "components",
      "shared",
      "node_modules"
    ],
    "mocksPattern": "(?:(?:[\\/]|^)__mocks__)|(?:\\/internals\\/testing\\/mocks)[\\/]",
    "globals": {
      "__CLIENT__": false,
      "__SERVER__": true,
      "__DEVELOPMENT__": false,
      "__PRODUCTION__": true,
      "__CARDS__": false
    }
  }
}
